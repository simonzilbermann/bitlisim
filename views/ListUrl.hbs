<style>

</style>
        <div id="data"></div>  
        <script>
            str="";
            fetch('/press',{
                method: 'GET'
                }).then((res)=>{
                    return res.json()
                }).then((data)=>{ 
                    document.writeln('<body style="background: linear-gradient(110deg, #fdcd3b 60%, #ffed4b 60%);">');
                    document.writeln('<table><thead><tr><th>Press URL</th><th>Count</th><th>URL</th></tr></thead>');
                    for(let i=0;i<data.length;i++)
                    {
                        document.writeln('<tbody><tr><td style="color: blue"><hr>'+ window.location + 's/' +data[i].Urlres +'</td><td><hr>'+ data[i].Count +'</td><td style="color: red;"><hr>'+ data[i].Url +'</td></tr></tbody>');
                    }
            });      
            str += '</table></body>';
            document.getElementById('data').innerHTML=str;        
        </script>
</html>

